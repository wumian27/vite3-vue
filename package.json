{
  "name": "vue-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite --mode dev",
    "dev": "vue-tsc --noEmit && vite build --mode dev",
    "test": "vue-tsc --noEmit && vite build --mode test",
    "ywuat": "vue-tsc --noEmit && vite build --mode ywuat",
    "build": "vue-tsc --noEmit && vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.tsx --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json.md,less}\""
  },
  "dependencies": {
    "axios": "^0.27.2",
    "element-plus": "^2.2.13",
    "sass": "^1.54.4",
    "vue": "^3.2.25",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-vue": "^2.3.4",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^2.9.15",
    "vite-plugin-environment": "^1.1.2",
    "vite-plugin-externals": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inject-externals": "^1.1.1",
    "vite-plugin-mock": "^2.9.6",
    "vue-tsc": "^0.34.7"
  }
}
